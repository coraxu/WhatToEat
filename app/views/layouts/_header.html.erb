<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
  <% if logged_in? %>
    <%= link_to "WhatToEat app", homepage2_path, id: "logo" %>
  <% else %>
    <%= link_to "WhatToEat app", homepage_path, id: "logo" %>
  <% end %>
    <nav>
      <ul class="nav navbar-nav navbar-right">
        <% if logged_in? %>
          <li><%= link_to "Home", homepage2_path %></li>
          <% if check_admin %>
              <li><%= link_to "App", app_path %></li>
              <li><%= link_to "Profile", '#' %></li>
              <li><%= link_to "Settings", '#' %></li>
              <li><%= link_to "Log out", logout_path, method: :delete %></li>
              <li><%= link_to "Users", users_path %></li>
          <% else %>
              <li><%= link_to "App", app_path %></li>
              <li><%= link_to "Profile", '#' %></li>
              <li><%= link_to "Settings", 'https://www.facebook.com' %></li>
              <li><%= link_to "Log out", logout_path, method: :delete %></li>
          <% end %>
        <% else %>
          <li><%= link_to "Home", homepage_path %></li>
          <li><%= link_to "Log in", login_path %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>