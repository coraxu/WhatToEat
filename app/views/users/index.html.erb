<% provide(:title, 'Existing Users') %>
<h1>List of Users</h1>

<ul class="users">
  <% @users.each do |user| %>
  <li>
    <%= link_to user.name, user %>
    ||<%= link_to "delete", user, method: :delete, data: 
    { confirm: "Are You Sure You Want To Delte this User?"}>
  </li>
  <% end %>
</ul>
